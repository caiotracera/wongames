{
  "extends": ["config:base"],
  "semanticCommits": true,
  "stabilityDays": 3,
  "prCreation": "not-pending",
  "labels": ["type: dependencies"],
  "postUpgradeTasks": {
    "commands": ["npm install --force"],
    "fileFilters": ["package-lock.json", "**/*.ts", "**/*.tsx"],
    "executionMode": "update"
  },
  "packageRules": [
    {
      "packageNames": ["node"],
      "enabled": false
    }
  ]
}
